{
  "name": "web",
  "version": "1.0.0",
  "description": "Project web",
  "main": "index.js",
  "scripts": {
    "start:client": "webpack serve --config ./config/webpack/config.dev.js --host 0.0.0.0 --port 3001 --disable-host-check",
    "build:client": "rm -fr ./out && NODE_ENV=build webpack --config ./config/webpack/config.prod.js",
    "start:server": "nodemon --watch src/server --ext tsx,ts --exec ts-node src/server/index.ts",
    "build:server": "rm -fr ./dist && NODE_ENV=build tsc",
    "start": "npm run start:server & npm run start:client",
    "build": "npm run build:server && npm run build:client",
    "prepublish": "pm2 stop $PROJECT_NAME || true",
    "publish": "NODE_ENV=build pm2 start src/index.js --name $PROJECT_NAME"
  },
  "author": "Jhonnatan Castro",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pm2": "^4.5.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.3",
    "require-from-string": "^2.0.2",
    "memory-fs": "^0.5.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "dotenv-expand": "^5.1.0",
    "@types/express": "^4.17.2",
    "@types/mysql": "^2.15.8",
    "@types/node": "^13.1.1",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "ts-node": "^8.5.4",
    "ts-loader": "^8.0.18",
    "url-loader": "^4.1.1",
    "file-loader": "^6.2.0",
    "typescript": "^3.7.4",
    "node-fetch": "^2.6.1",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^2.5.2",
    "html-webpack-plugin": "^5.3.1"
  }
}
